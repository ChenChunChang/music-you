{
  "name": "vplayer-next",
  "version": "2.0.0-beta.2",
  "private": true,
  "main": "dist/main/index.cjs",
  "author": "gumengyu",
  "description": "A 163 netease music player (base on vue@3)",
  "scripts": {
    "preview": "vite preview",
    "dev": "vite --config packages/renderer/vite.config.ts",
    "build": "vite build --config packages/renderer/vite.config.ts",
    "electron:dev": "node scripts/watch.mjs",
    "electron:build": "npm run electron:prebuild && electron-builder",
    "electron:build:mac": "npm run electron:prebuild && electron-builder --mac",
    "electron:build:win": "npm run electron:prebuild && electron-builder --win",
    "electron:build:linux": "npm run electron:prebuild && electron-builder --linux",
    "electron:prebuild": "node scripts/build.mjs",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:ts",
    "lint:eslint": "eslint \"packages/renderer/src/**/*.{js,jsx,ts,tsx,vue}\" --fix",
    "lint:prettier": "prettier --write \"packages/renderer/src/**/*.{js,tsx,css,scss,vue,md,json,html}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unblockneteasemusic/server": "^0.27.0-rc.6",
    "dotenv": "^16.0.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.7",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.6",
    "NeteaseCloudMusicApi": "git+https://gitee.com/YuSaKiYo/NeteaseCloudMusicApi.git#unlock"
  },
  "devDependencies": {
    "@mdi/js": "^6.5.95",
    "@types/express": "^4.17.13",
    "@types/howler": "^2.2.7",
    "@types/lodash-es": "^4.17.6",
    "@types/md5": "^2.3.2",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vuetify/vite-plugin": "^1.0.0-alpha.0",
    "@vueuse/components": "^8.4.2",
    "@vueuse/core": "^8.2.5",
    "@vueuse/electron": "^8.3.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.1",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "electron-dl": "^3.3.1",
    "electron-is": "^3.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.7.0",
    "howler": "^2.2.3",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.9.2",
    "md3-theme-generator": "^0.0.1",
    "md5": "^2.3.0",
    "overlayscrollbars": "^1.13.1",
    "pinia": "^2.0.13",
    "prettier": "^2.6.2",
    "sass": "^1.38.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.8.0",
    "vite-plugin-electron": "^0.4.3",
    "vite-plugin-resolve": "^2.0.7",
    "vue": "^3.2.25",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-tsc": "^0.34.11",
    "vue-virtual-scroller": "^2.0.0-alpha.1",
    "vue3-slider": "^1.6.9",
    "vuetify": "^3.0.0-beta.3",
    "vuetify-ctx-menu": "git+https://github.com/GuMengYu/vue3-context-menu.git",
    "webfontloader": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
